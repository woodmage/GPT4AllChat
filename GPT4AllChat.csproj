  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="7.0.1" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="Gpt4All">
      <HintPath>..\..\gpt4all\gpt4all\gpt4all-bindings\csharp\Gpt4All\bin\Debug\net6.0\Gpt4All.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>